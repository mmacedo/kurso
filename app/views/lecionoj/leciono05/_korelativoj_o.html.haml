%h3#korelativoj_o -o
%p= t '.ekspliko'
%dl.dl-horizontal
  %dt ki-
  %dd
    %dl.dl-horinzontal
      %dt.eo kio
      %dd= t '.ki.vorto'
      %dt.eo Kio estas geografio? — Geografio estas scienco.
      %dd= t '.ki.frazo'
  %dt ti-
  %dd
    %dl.dl-horinzontal
      %dt.eo tio
      %dd= t '.ti.vorto'
      %dt.eo Kio estas tio? — Tio estas muzikilo.
      %dd= t '.ti.frazo'
  %dt i-
  %dd
    %dl.dl-horinzontal
      %dt.eo io
      %dd= t '.i.vorto'
      %dt.eo Io okazis!
      %dd= t '.i.frazo'
  %dt neni-
  %dd
    %dl.dl-horinzontal
      %dt.eo nenio
      %dd= t '.neni.vorto'
      %dt.eo Pli bona io, ol nenio.
      %dd= t '.neni.frazo'
  %dt ĉi-
  %dd
    %dl.dl-horinzontal
      %dt.eo ĉio
      %dd= t '.ĉi.vorto'
      %dt.eo Mi forgesis ĉion.
      %dd= t '.ĉi.frazo'
